<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:flowable="http://flowable.org/bpmn"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI"
             targetNamespace="http://www.flowable.org/processdef">

  <process id="demoIntakeProcess" name="ðŸš€ AI-Powered Intake (Demo)" isExecutable="true">

    <documentation>AI-Powered Intelligent Intake Process - Full Pipeline Demo</documentation>

    <!-- ==================== START ==================== -->
    <startEvent id="start" name="ðŸ“¥ Customer Request"/>
    <sequenceFlow id="f1" sourceRef="start" targetRef="sourceGateway"/>

    <!-- ==================== SOURCE GATEWAY ==================== -->
    <exclusiveGateway id="sourceGateway" name="ðŸ“ž Audio / Text?"/>

    <sequenceFlow id="audioPath" name="Audio" sourceRef="sourceGateway" targetRef="speechToText">
      <conditionExpression xsi:type="tFormalExpression">${source == 'phone' || source == 'audio'}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="textPath" name="Text" sourceRef="sourceGateway" targetRef="sentimentAnalysis">
      <conditionExpression xsi:type="tFormalExpression">${source == 'text' || source == 'form' || source == 'email'}</conditionExpression>
    </sequenceFlow>

    <!-- ==================== AUDIO BRANCH ==================== -->
    <serviceTask id="speechToText" name="ðŸŽ¤ Speech-to-Text" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>STT complete</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <serviceTask id="parseTranscription" name="ðŸ“ Parse Transcription" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>Parse complete</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <sequenceFlow id="f_stt_parse" sourceRef="speechToText" targetRef="parseTranscription"/>
    <sequenceFlow id="f_parse_sent" sourceRef="parseTranscription" targetRef="sentimentAnalysis"/>

    <!-- ==================== AI PIPELINE ==================== -->
    <serviceTask id="sentimentAnalysis" name="ðŸ˜Š Sentiment Analysis" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>Sentiment analyzed</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <serviceTask id="generateEmbedding" name="ðŸ§® Generate Embedding" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>Embedding done</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <serviceTask id="qdrantSearch" name="ðŸ” Qdrant Search" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>Search done</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <serviceTask id="rerankResults" name="ðŸ”„ Rerank Results" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>Rerank done</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <serviceTask id="buildRagContext" name="ðŸ“š Build RAG Context" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>RAG context built</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <serviceTask id="llmDecision" name="ðŸ¤– Gemini LLM Decision" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>LLM decided</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <serviceTask id="subCategorization" name="ðŸ¢ Sub-Categorization" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>Sub-category set</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <!-- Pipeline flow -->
    <sequenceFlow id="f_sent_emb" sourceRef="sentimentAnalysis" targetRef="generateEmbedding"/>
    <sequenceFlow id="f_emb_qs" sourceRef="generateEmbedding" targetRef="qdrantSearch"/>
    <sequenceFlow id="f_qs_rr" sourceRef="qdrantSearch" targetRef="rerankResults"/>
    <sequenceFlow id="f_rr_rag" sourceRef="rerankResults" targetRef="buildRagContext"/>
    <sequenceFlow id="f_rag_llm" sourceRef="buildRagContext" targetRef="llmDecision"/>
    <sequenceFlow id="f_llm_sub" sourceRef="llmDecision" targetRef="subCategorization"/>
    <sequenceFlow id="f_sub_par" sourceRef="subCategorization" targetRef="parallelSplit"/>

    <!-- ==================== PARALLEL MCP TOOLS ==================== -->
    <parallelGateway id="parallelSplit" name="âš™ï¸ Execute MCP Tools"/>

    <serviceTask id="updateCategory" name="ðŸ“ Update Category" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>Category updated</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <serviceTask id="setPriority" name="âš¡ Set Priority" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>Priority set</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <serviceTask id="createTask" name="ï¿½ Create Task" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>Task created</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <sequenceFlow id="f_par_cat" sourceRef="parallelSplit" targetRef="updateCategory"/>
    <sequenceFlow id="f_par_pri" sourceRef="parallelSplit" targetRef="setPriority"/>
    <sequenceFlow id="f_par_task" sourceRef="parallelSplit" targetRef="createTask"/>

    <parallelGateway id="parallelJoin" name="âš™ï¸ Join"/>

    <sequenceFlow id="f_cat_join" sourceRef="updateCategory" targetRef="parallelJoin"/>
    <sequenceFlow id="f_pri_join" sourceRef="setPriority" targetRef="parallelJoin"/>
    <sequenceFlow id="f_task_join" sourceRef="createTask" targetRef="parallelJoin"/>

    <sequenceFlow id="f_join_route" sourceRef="parallelJoin" targetRef="priorityRouting"/>

    <!-- ==================== PRIORITY ROUTING ==================== -->
    <exclusiveGateway id="priorityRouting" name="âš¡ Priority?"/>

    <sequenceFlow id="urgentFlow" name="URGENT" sourceRef="priorityRouting" targetRef="urgentTask">
      <conditionExpression xsi:type="tFormalExpression">${aiPriority == 'URGENT'}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="highFlow" name="HIGH" sourceRef="priorityRouting" targetRef="highTask">
      <conditionExpression xsi:type="tFormalExpression">${aiPriority == 'HIGH'}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="mediumFlow" name="MEDIUM" sourceRef="priorityRouting" targetRef="mediumTask">
      <conditionExpression xsi:type="tFormalExpression">${aiPriority == 'MEDIUM'}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="lowFlow" name="LOW" sourceRef="priorityRouting" targetRef="lowTask">
      <conditionExpression xsi:type="tFormalExpression">${aiPriority == 'LOW' || (aiPriority != 'URGENT' &amp;&amp; aiPriority != 'HIGH' &amp;&amp; aiPriority != 'MEDIUM')}</conditionExpression>
    </sequenceFlow>

    <!-- ==================== USER TASKS ==================== -->
    <userTask id="urgentTask" name="ðŸ”´ URGENT: ${aiCategory}" flowable:candidateGroups="emergency-team">
      <documentation>ðŸ”´ URGENT - Customer: ${customerId} | ${question} | Intent: ${intent}</documentation>
    </userTask>
    <userTask id="highTask" name="ðŸŸ  HIGH: ${aiCategory}" flowable:candidateGroups="managers">
      <documentation>ðŸŸ  HIGH - Customer: ${customerId} | ${question} | Intent: ${intent}</documentation>
    </userTask>
    <userTask id="mediumTask" name="ðŸŸ¢ MEDIUM: ${aiCategory}" flowable:candidateGroups="support-team">
      <documentation>ðŸŸ¢ MEDIUM - Customer: ${customerId} | ${question} | Intent: ${intent}</documentation>
    </userTask>
    <userTask id="lowTask" name="ðŸ”µ LOW: ${aiCategory}" flowable:candidateGroups="support-team">
      <documentation>ðŸ”µ LOW - Customer: ${customerId} | ${question} | Intent: ${intent}</documentation>
    </userTask>

    <!-- ==================== STORE & END ==================== -->
    <exclusiveGateway id="taskJoin"/>

    <sequenceFlow id="f_urg_join" sourceRef="urgentTask" targetRef="taskJoin"/>
    <sequenceFlow id="f_high_join" sourceRef="highTask" targetRef="taskJoin"/>
    <sequenceFlow id="f_med_join" sourceRef="mediumTask" targetRef="taskJoin"/>
    <sequenceFlow id="f_low_join" sourceRef="lowTask" targetRef="taskJoin"/>

    <serviceTask id="storeVectorDb" name="ðŸ’¾ Store to Vector DB" flowable:type="shell">
      <extensionElements>
        <flowable:field name="command"><flowable:string>echo</flowable:string></flowable:field>
        <flowable:field name="arg1"><flowable:string>Stored</flowable:string></flowable:field>
        <flowable:field name="wait"><flowable:string>true</flowable:string></flowable:field>
      </extensionElements>
    </serviceTask>

    <sequenceFlow id="f_taskjoin_store" sourceRef="taskJoin" targetRef="storeVectorDb"/>
    <sequenceFlow id="f_store_end" sourceRef="storeVectorDb" targetRef="end"/>

    <endEvent id="end" name="âœ… Complete"/>

  </process>

